	"enum_transitions":
		{
			"FoodType" : 
				{
					"vegetable" : "VegetableType",
					"fruit" : "FruitType",
					"juice" : "JuiceType",
					"tomatoproduct" : "TomatoProductType",
					"supplement" : "SupplementType"
				},
			"SupplementType" : 
				{
					"barcode" : "Barcode",
					"others$label" : "AmountVariable"
				},
			"VegetableType" : 
				{
					"*" : "VegetableState",
					"pumpkin" : "VegetableState+UseWeight",
					"corn" : "VegetableState+UseWeight",
					"salad_specific" : "SaladType",
					"spinach" : "VegetableState+UseWeight",
					"curlykale" : "VegetableState+UseWeight",
					"collards" : "VegetableState+UseWeight",
					"beans_peas" : "VegetableState+UseWeight",
					"red_cabbage" : "VegetableState+UseWeight",
					"paprika" : "PaprikaColor"
				},
			"SaladType" : 
				{
					"*" : "AmountGramms"			
				},
			"PaprikaColor" : 
				{
					"*" : "VegetableState"			
				},
			"VegetableState" : 
				{
					"*" : "AmountVariable",
					"*+UseWeight" : "AmountGramms"					
				},
			"FruitState" : 
				{
					"*" : "AmountVariable"				
				},
			"FruitType" : 
				{
					"*" : "FruitState",
					"citrus": "CitrusType"
				},
			"CitrusType" : 
				{
					"*" : "FruitState"
				},
			"JuiceType" : 
				{
					"*" : "AmountLiquid"
				},
			"TomatoProductType" : 
				{
					"*" : "AmountVariable",
					"sieved" : "AmountGramms",
					"pasta" : "AmountGramms",
					"soup" : "AmountLiquid",
					"ketchup" : null,
					"paste" : null
				},
			"Barcode" : 
				{
					"*" : "AmountVariable"
				},
			"AmountVariable":
				{
					"gramms" : "AmountGramms",
					"pieces" : "AmountPieces"
				}
		}
